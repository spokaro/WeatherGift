platform :ios, '10.0'

sources 
	'https://github.com/CocoaPods/Specs.git' 
	'https://github.com/SwiftyJSON/SwiftyJSON' 
	'https://github.com/Alamofire/Alamofire' 
	'https://cocoapods.org/pods/GooglePlacesAPI'

target 'WeatherGift' do
  use_frameworks!
    pod 'GooglePlaces'
    pod 'GoogleMaps'
    pod 'GooglePlacesAPI', '~> 1.1'
    pod 'Alamofire', '~> 4.0'
    pod 'SwiftyJSON', '~> 4.0'
end
